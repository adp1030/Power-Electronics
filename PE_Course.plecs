    Component {
      Type          Subsystem
      Name          "Switch Cell T2\nVoltage Mode1"
      Show          on
      Position      [95, 170]
      Direction     up
      Flipped       off
      Frame         [-40, -25; 40, 25]
      LabelPosition [0, -28]
      LabelAlign    down
      SampleTime    "-1"
      CodeGenDiscretizationMethod "2"
      CodeGenTarget "Generic"
      MaskType      "Switch Cell Voltage Mode (Type 2)"
      MaskDisplay   "local CellType = 2;\n"
"\n"
"Icon:line({0,0}, {15, 10})\n"
"Icon:line({0,5}, {10, 0})\n"
"Icon:line({0,0}, {0, -10})\n"
"\n"
"Icon:circle(0,-10, 1)\n"
"\n"
"Icon:line({0,-30}, {-10, -10})\n"
"Icon:line({0,10}, {-10, -10})\n"
"Icon:line({20,30}, {-10, -10})\n"
"\n"
"Icon:line({10,10}, {-15, -5})\n"
"Icon:line({20,20}, {-15, -5})\n"
"if CellType == 2 then\n"
"\tIcon:line({10,20}, {-05, -10})\n"
"\tIcon:line({10,20}, {-15, -10})\n"
"else\n"
"\tIcon:line({20,10}, {-05, -10})\n"
"\tIcon:line({20,10}, {-15, -10})\n"
"end\n"
      MaskDisplayLang "2"
      MaskIconFrame on
      MaskIconOpaque off
      MaskIconRotates on
      MaskInit      "% cell-type\n"
"CellType = 1;\n"
"\n"
"Ts = 1/Fpwm;\n"
"Rdson = 1e-3;\n"
"\n"
"if Type == 1\n"
"\t% Buck Converter\n"
"\tLcrit = Ts/2*R*(1-M);\n"
"\talpha = L/Lcrit;\n"
"\tif alpha < 1\n"
"\t\tD = sqrt(alpha)*M;\n"
"  \telse\n"
"\t\tD = M;\n"
"  \tend\n"
"\tRe = 2*L/(D^2*Ts);\n"
"\tV1 = 1; % using actual (averaged) measurement\n"
"\tg1 = -1/Re;\n"
"\tj1 = 2*(1-M)*V1/D/Re;\n"
"\tr1 = Re;\n"
"\tg2 = (2-M)/M/Re;\n"
"\tj2 = 2*(1-M)*V1/D/M/Re;\n"
"\tr2 = M^2*Re;\n"
"elseif Type == 2\n"
"\t% Boost Converter\n"
"\tLcrit = Ts/2*R*(1-1/M)/M^2;\n"
"\talpha = L/Lcrit;\n"
"\tif alpha < 1\n"
"\t\tK = 2/Ts*L/R;\n"
"\t\tD = sqrt(K)*sqrt(1-1/M)*M;\n"
"  \telse\n"
"\t\tD = 1-1/M;\n"
"  \tend\n"
"  \tRe = 2*L/(D^2*Ts);\n"
"\tV1 = 1; % using actual (averaged) measurement\n"
"  \tg1 = -1/(M-1)^2/Re;\n"
"\tj1 = 2*M*V1/D/(M-1)/Re;\n"
"\tr1 = (M-1)^2/M^2*Re;\n"
"\tg2 = (2*M-1)/(M-1)^2/Re;\n"
"\tj2 = 2*V1/D/(M-1)/Re;\n"
"\tr2 = (M-1)^2*Re;\n"
"elseif Type == 3\n"
"\t% Buck-Boost Converter\n"
"\tLcrit = R*Ts/2*1/(M+1)^2;\n"
"\talpha = L/Lcrit;\n"
"\tif alpha < 1\n"
"\t\tD = sqrt(alpha)*M/(M+1);\n"
"  \telse\n"
"\t\tD = M/(M+1);\n"
"  \tend\n"
"  \tRe = 2*L/(D^2*Ts);\n"
"end\n"
"\n"
"if Model == 1\n"
"\tImplementation = 1; % switched\n"
"elseif Model == 2\n"
"\tImplementation = 2; % transformer (large signal CCM)\n"
"else\n"
"\t% small signal\n"
"   if alpha >= 1\n"
"   \tImplementation = 3;\n"
"\telseif Type == 1\n"
"\t\tImplementation = 4;\n"
"\telseif Type == 2\n"
"\t\tImplementation = 5;\n"
"\telseif Type == 3\n"
"\t\tImplementation = 6;\n"
"\tend\n"
"end\n"
"\n"
"\n"
"\n"
      MaskDialogCallback "Dialog:set('Type', 'Visible', Dialog:get('Model') =="
" '3')\n"
"Dialog:set('Fpwm', 'Visible', Dialog:get('Model') ~= '2')\n"
"Dialog:set('M', 'Visible', Dialog:get('Model') == '3')\n"
"Dialog:set('L', 'Visible', Dialog:get('Model') == '3')\n"
"Dialog:set('R', 'Visible', Dialog:get('Model') == '3')"
